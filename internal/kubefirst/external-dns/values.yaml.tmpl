image:
  repository: registry.k8s.io/external-dns/external-dns
  tag: "v0.13.2"
serviceAccount:
  create: true
  name: external-dns
provider: {{ .Provider }}
sources:
- ingress
domainFilters:
- {{ .DomainName }}
env:
- name: {{ .EnvName }}
  valueFrom:
    secretKeyRef:
      name: external-dns-secrets
      key: token
